+function myFunc() {
+    let udp_ip = document.getElementById("ip_udp");
+    let err = document.getElementById("err");
+    let udp = udp_ip.value.split('.');
+    console.log(udp);
+    if ((Number(udp[0]) >= "224" && Number(udp[0]) <= "239") &&
+        (Number(udp[1]) >= 0 && Number(udp[1]) <= 254) &&
+        (Number(udp[2]) >= 0 && Number(udp[2]) <= 254) &&
+        (Number(udp[3]) > 0 && Number(udp[3]) < 254))
+        {
+           console.log("OK");
+           err.hidden = true;
+    }
+    else {
+        err.hidden = false;
+        console.log("Error");
+    }
+}
